{
  "LogQueries": false,
  "AllowedHosts": "*",
  "AllowedOrigin": "http://localhost:3000",
  "Witsml": {
    "ClientCapabilities": {
      "Name": "Witsml Explorer",
      "Description": "Browser interface for Witsml servers"
    }
  },
  "OAuth2Enabled": false,
  "enableHttp": false,
  "TempDataStorageDir": "d:\\tmp\\witsml",
  "TempFilesRetentionHours": 1,
  "LogFilesRetentionHours": 8,
  "OpenAiLogDir": "d:\\tmp\\witsml\\logs",
  "OpenAiApiKey": "YOUR_OPENAI_API_KEY",
  "OpenAiModelName": "gpt-4o-mini",
  "PythonContainerMaxExecTimeSec": 20
}
